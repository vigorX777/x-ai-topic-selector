{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "types": ["bun-types"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
